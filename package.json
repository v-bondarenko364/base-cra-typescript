{
	"name": "cra-boilerplate",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"web:start:dev": "react-scripts start",
		"web:build": "react-scripts build",
		"prettier": "prettier typescript --write 'src/**/*.{ts, tsx}'",
		"lint": "yarn lint:js && yarn lint:css && yarn lint:types",
		"lint:js": "eslint src/**/*.{ts,tsx}",
		"lint:css": "stylelint './src/**/*.tsx'",
		"lint:types": "tsc",
		"lint:last-commit": "commitlint --from=HEAD~1",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"dependencies": {
		"@material-ui/core": "^4.5.1",
		"@material-ui/icons": "^4.5.1",
		"@types/node": "12.7.12",
		"@types/styled-components": "^4.1.19",
		"axios": "^0.19.0",
		"react": "^16.10.2",
		"react-dom": "^16.10.2",
		"react-redux": "^7.1.1",
		"react-scripts": "3.2.0",
		"redux": "^4.0.4",
		"redux-saga": "^1.1.1",
		"styled-components": "^4.4.0",
		"styled-components-breakpoint": "^3.0.0-preview.20",
		"typesafe-actions": "^4.4.2",
		"typescript": "3.6.4"
	},
	"eslintIgnore": [
		"src/serviceWorker.ts",
		"src/tests"
	],
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^8.2.0",
		"@types/jest": "^24.0.18",
		"@types/react": "^16.9.5",
		"@types/react-dom": "^16.9.1",
		"@types/react-redux": "^7.1.4",
		"@typescript-eslint/eslint-plugin": "^2.3.3",
		"@typescript-eslint/parser": "^2.3.2",
		"commitlint": "^8.2.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-prettier": "^6.4.0",
		"eslint-plugin-prettier": "^3.1.1",
		"husky": "^3.0.8",
		"lint-staged": "^9.4.2",
		"prettier": "^1.19.1",
		"pretty-quick": "^2.0.0",
		"styled-reset": "^4.0.7",
		"stylelint": "^11.1.1",
		"stylelint-config-recommended": "^3.0.0",
		"stylelint-config-styled-components": "^0.1.1",
		"stylelint-processor-styled-components": "^1.8.0"
	}
}
